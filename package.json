{
  "name": "metatron",
  "type": "module",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/chroniclehq/metatron#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chroniclehq/metatron.git"
  },
  "bugs": "https://github.com/chroniclehq/metatron/issues",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
